# Prometheus v4.0 配置文件模板
# 复制此文件为 .env 并填入真实的API密钥

# 交易数据源选择
# mock: 模拟数据（快速调试，无需网络，价格随机生成）
# okx: OKX模拟盘（真实环境测试，需要API密钥）
TRADING_MODE=mock

# OKX API 配置（仅在TRADING_MODE=okx时需要）
OKX_API_KEY=your_api_key_here
OKX_API_SECRET=your_api_secret_here
OKX_PASSPHRASE=your_passphrase_here

# 交易模式 (True=模拟盘, False=实盘)
OKX_SANDBOX=True

# 日志级别 (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

